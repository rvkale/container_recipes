#%Module -*- tcl -*-
##
## modulefile
##

set              name              cub
set              version           1.8.0
set              root              /usr/local/${name}/${version}

proc ModulesHelp { } {

  puts stderr "\tAdds $name (version $version) to your environment variables"
}

module-whatis "adds $name (version $version) to your environment variables."

setenv            CUB_ROOT          ${root}
